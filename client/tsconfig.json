{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "target": "es6",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "esnext"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": [
      "react",
      "react-dom"
    ]
  },
  "include": [
    "src",
    "vite-env.d.ts",
    "../shared"
  ],
  "exclude": [
    "node_modules"
  ]
}